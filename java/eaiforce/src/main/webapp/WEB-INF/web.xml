<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>eaiforce</display-name>
  <context-param>
    <description>Database Environment</description>
    <param-name>env</param-name>
    <param-value>dev</param-value>
  </context-param>
  <context-param>
    <description>Upload Temp Directory</description>
    <param-name>temp-directory</param-name>
    <param-value>/Users/sateav/Downloads/upload/</param-value>
  </context-param>
  <context-param>
    <description>Page Size</description>
    <param-name>pageSize</param-name>
    <param-value>25</param-value>
  </context-param>
  <context-param>
    <description>Search Page Size</description>
    <param-name>searchPageSize</param-name>
    <param-value>25</param-value>
  </context-param>
  <context-param>
    <description>Aspose License File</description>
    <param-name>aspose-license</param-name>
    <param-value>/Users/sateav/Downloads/Aspose.Pdf.lic</param-value>
  </context-param>
  <servlet>
    <servlet-name>home</servlet-name>
    <jsp-file>/main.jsp</jsp-file>
