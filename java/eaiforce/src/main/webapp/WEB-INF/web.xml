    <jsp-file>/mainForm.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>prospecto</servlet-name>
    <url-pattern>/prospecto</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>buscar</servlet-name>
    <jsp-file>/mainSearch.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>buscar</servlet-name>
    <url-pattern>/buscar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>notificaciones</servlet-name>
    <jsp-file>/notifications.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>notificaciones</servlet-name>
    <url-pattern>/notificaciones</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>administracion</servlet-name>
    <jsp-file>/mainAdmin.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>administracion</servlet-name>
    <url-pattern>/administracion</url-pattern>
