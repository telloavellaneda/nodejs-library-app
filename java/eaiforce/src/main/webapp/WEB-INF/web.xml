    <jsp-file>/invalidateSession.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>cerrar</servlet-name>
    <url-pattern>/cerrar</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>UpsertCustomerServlet</servlet-name>
    <servlet-class>force.eai.mx.servlet.UpsertCustomerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UpsertCustomerServlet</servlet-name>
    <url-pattern>/upsertCustomer</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>LoadCustomerServlet</servlet-name>
    <servlet-class>force.eai.mx.servlet.LoadCustomerServlet</servlet-class>
  </servlet>
