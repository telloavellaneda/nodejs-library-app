SELECT FECHA 
FROM
(
	SELECT
	TO_CHAR(C.{1}, ''YYYY'') FECHA
	,COUNT(*)
	FROM
		EAI_ADM_USUARIOS A
		,EAI_REL_CLIENTES B
		,EAI_REL_FECHAS C
	WHERE 1 = 1
		AND ID_CLIENTE = {0}
		AND C.{1} IS NOT NULL
		AND A.ID_USUARIO = B.ID_USUARIO
		AND B.FORCE_ID = C.FORCE_ID
	GROUP BY TO_CHAR(C.{1}, ''YYYY'')
)
UNION
SELECT TO_CHAR(FECHA,''YYYY'')
FROM
(
	SELECT TRUNC(SYSDATE,''YYYY'') FECHA FROM DUAL
	UNION
	SELECT ADD_MONTHS(TRUNC(SYSDATE,''YYYY''),-12) FECHA FROM DUAL
	UNION
	SELECT ADD_MONTHS(TRUNC(SYSDATE,''YYYY''),-24) FECHA FROM DUAL
	UNION
	SELECT ADD_MONTHS(TRUNC(SYSDATE,''YYYY''),-36) FECHA FROM DUAL
	UNION
	SELECT ADD_MONTHS(TRUNC(SYSDATE,''YYYY''),-48) FECHA FROM DUAL
	UNION
	SELECT ADD_MONTHS(TRUNC(SYSDATE,''YYYY''),-60) FECHA FROM DUAL
)
ORDER BY FECHA DESC
